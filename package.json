{
  "name": "nios-ii",
  "displayName": "Nios II",
  "description": "Nios II Language Support for Nios II Assembly Language Programming",
  "publisher": "niosii",
  "version": "0.0.15",
  "repository": "https://github.com/estKey/nios-ii",
  "engines": {
    "vscode": "^1.36.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:niosii"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.36.0",
    "typescript": "^4.0.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "niosii",
        "aliases": [
          "Nios II",
          "niosii",
          "niosiiasm",
          "niosii asm"
        ],
        "extensions": [
          ".s",
          ".niosii",
          ".eds",
          ".elf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "niosii",
        "path": "./snippets/niosiiasm.json"
      },
      {
        "language": "niosii",
        "path": "./snippets/niosiisudo.json"
      }
    ],
    "grammars": [
      {
        "language": "niosii",
        "scopeName": "source.s",
        "path": "./syntaxes/niosii.assembly.tmLanguage.json"
      }
    ]
  }
}
